{
	"name": "Python & MySQL Dev Environment",
	"dockerComposeFile": ["docker-compose.yml"],
	"service": "app",
	"workspaceFolder": "/workspace",
	"postCreateCommand": "pip install -r requirements.txt",
	"forwardPorts": [3306],
	"features": {
	  "docker-in-docker": "latest"
	},
	"customizations": {
	  "vscode": {
		"extensions": [
		  "ms-python.python",
		  "ms-python.vscode-pylance",
		  "mhutchie.git-graph"
		]
	  }
	}
  }